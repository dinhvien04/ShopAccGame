{
    "name": "quanlychitieuroot",
    "version": "1.0.0",
    "description": "Game Account Marketplace - Full Stack Application",
    "main": "index.js",
    "scripts": {
        "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
        "dev:backend": "cd backend && npm start",
        "dev:frontend": "cd frontend-new && npm run dev",
        "install:all": "npm install && cd backend && npm install && cd ../frontend-new && npm install",
        "build": "cd frontend-new && npm run build",
        "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
        "start:backend": "cd backend && npm start",
        "start:frontend": "cd frontend-new && npm start"
    },
    "keywords": [
        "game",
        "marketplace",
        "react",
        "nextjs",
        "nodejs",
        "express",
        "mongodb"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}